            android:name="com.project.myapplication.exchange"
            android:theme="@style/Theme.Material3.Light.NoActionBar" />
        <activity
            android:name="com.project.myapplication.CitydetailsActivity"
            android:theme="@style/Theme.Material3.Light.NoActionBar" />
        <activity
            android:name="com.project.myapplication.Map"
            android:theme="@style/Theme.Material3.Light.NoActionBar" />
        <activity
            android:name="com.project.myapplication.MainActivity"
            android:exported="false"
            android:theme="@style/Theme.Material3.Light.NoActionBar" />
    </application>

</manifest>