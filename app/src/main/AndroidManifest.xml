<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/city"
        android:label="@string/app_name"
        android:roundIcon="@drawable/city"
        android:supportsRtl="true"
        android:theme="@style/Theme.Material3.Light">
        <activity
            android:name="com.project.myapplication.start"
            android:exported="true"
            android:theme="@style/Theme.Material3.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.project.myapplication.Gmap"
            android:exported="false"
            android:theme="@style/Theme.Material3.Light.NoActionBar" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/my_map_api_key" />

        <activity
            android:name="com.project.myapplication.feedback"
            android:exported="false"
            android:theme="@style/Theme.Material3.Light.NoActionBar" />
        <activity
            android:name="com.project.myapplication.About"
            android:exported="false"
            android:theme="@style/Theme.Material3.Light.NoActionBar" />
        <activity
            android:name="com.project.myapplication.Register"
            android:exported="false"
            android:theme="@style/Theme.Material3.Light.NoActionBar" />
        <activity
            android:name="com.project.myapplication.Login"
            android:exported="false"
            android:theme="@style/Theme.Material3.Light.NoActionBar" />
        <activity
            android:name="com.project.myapplication.HomePage"
            android:exported="false"
            android:theme="@style/Theme.Material3.Light.NoActionBar" />
        <activity
            android:name="com.project.myapplication.Info"
            android:exported="false"
            android:theme="@style/Theme.Material3.Light.NoActionBar" />
        <activity
            android:name="com.project.myapplication.HistoryActivity"
            android:theme="@style/Theme.Material3.Light.NoActionBar" />
        <activity
            android:name="com.project.myapplication.BusstationActivity"
            android:theme="@style/Theme.Material3.Light.NoActionBar" />
        <activity
            android:name="com.project.myapplication.PlacesActivity"
            android:theme="@style/Theme.Material3.Light.NoActionBar" />
        <activity
            android:name="com.project.myapplication.RestaurantActivity"
            android:theme="@style/Theme.Material3.Light.NoActionBar" />
